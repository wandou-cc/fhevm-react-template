{
  "name": "@fhevm-sdk",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "description": "Universal FHEVM SDK - Framework-agnostic SDK for Fully Homomorphic Encryption",
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "zama",
    "web3",
    "blockchain",
    "privacy"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./src/core/index.ts",
      "default": "./dist/core/index.js"
    },
    "./storage": {
      "types": "./src/storage/index.ts",
      "default": "./dist/storage/index.js"
    },
    "./adapters": {
      "types": "./src/adapters/index.ts",
      "default": "./dist/adapters/index.js"
    },
    "./adapters/ethers": {
      "types": "./src/adapters/ethers.ts",
      "default": "./dist/adapters/ethers.js"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./types/utils": {
      "types": "./src/types.utils.ts",
      "default": "./dist/types.utils.js"
    },
    "./errors": {
      "types": "./src/errors.ts",
      "default": "./dist/errors.js"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./dist/utils/index.js"
    },
    "./events": {
      "types": "./src/events.ts",
      "default": "./dist/events.js"
    },
    "./middleware": {
      "types": "./src/middleware.ts",
      "default": "./dist/middleware.js"
    },
    "./cache": {
      "types": "./src/cache.ts",
      "default": "./dist/cache.js"
    },
    "./plugin": {
      "types": "./src/plugin.ts",
      "default": "./dist/plugin.js"
    },
    "./testing": {
      "types": "./src/testing.ts",
      "default": "./dist/testing.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "idb": "^8.0.3"
  },
  "peerDependencies": {
    "@fhevm/mock-utils": "^0.1.0",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "ethers": "^6.13.4"
  },
  "devDependencies": {
    "@types/node": "~18.19.50",
    "@vitest/coverage-v8": "2.1.9",
    "@zama-fhe/relayer-sdk": "0.2.0",
    "ethers": "^6.13.7",
    "fake-indexeddb": "~6.0.0",
    "jsdom": "^27.0.0",
    "typescript": "~5.8.2",
    "vitest": "~2.1.8"
  }
}
